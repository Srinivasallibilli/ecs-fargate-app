{
  "family": "ecs-fargate-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::253862056441:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "fargate-container",
      "image": "253862056441.dkr.ecr.us-east-1.amazonaws.com/ecs-fargate-app:latest",
      "portMappings": [
        {
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "ENV",
          "value": "prod"
        }
      ]
    }
  ]
}
